// TODO: fix this test to use more updated crm key.
describe("Secrets Manager", () => {
  it("It should produce a secret hash that matches the hash on file", () => {
    cy.request(
      "http://localhost:5001/rumpelstiltskin-8cadf/us-central1/testBed?type=getSecret"
    ).then((val) => {
      const incomingValue = JSON.parse(val.body).res;
      const compareValue = {
        words: [
          -824057749, -1158421356, 406335700, 1992275050, -1256103063,
          696550254, 1360946367, -1952984095,
        ],
        sigBytes: 32,
      };
      expect(JSON.stringify(incomingValue)).to.eq(JSON.stringify(compareValue));
    });
  });
});
